#!/bin/bash
cd "$(dirname "$0")"

if [ -f "config.sh" ]; then
  source ./config.sh 1>/dev/null 2>/dev/null
fi

if [ -z "$REQUEST_TIMEOUT" ]; then REQUEST_TIMEOUT=30; fi
if [ -z "$WIT_API_VERSION" ]; then WIT_API_VERSION="27/06/2017"; fi

echo "{\"command\":\"conf\",\"api_version\":\"$WIT_API_VERSION\",\"api_token\":\"$WIT_API_TOKEN\",\"request_timeout\":$REQUEST_TIMEOUT}"
